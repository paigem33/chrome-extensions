{
    "name": "Text Link",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js", "thirdParty/jquery-3.4.1.min.js"]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "hello"
    },
    "permissions": [
        "storage"
    ],
    "background":
    {
        "scripts": ["thirdParty/jquery-3.4.1.min.js"]
    }
}